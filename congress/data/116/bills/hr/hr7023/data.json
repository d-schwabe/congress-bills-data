  "introduced_at": "2020-05-27", 
  "number": "7023", 
  "official_title": "To require the Secretary of the Treasury to implement a program that provides financial assistance to sports facilities, museums, and community theaters, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "bioguide_id": "T000482", 
    "district": "3", 
    "name": "Trahan, Lori", 
