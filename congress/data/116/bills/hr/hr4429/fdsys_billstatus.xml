      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2019-09-20</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2020-07-24T21:19:09Z</lastSummaryUpdateDate>
          <text><![CDATA[<p><b>Fairness to Kids with Cancer Act of 2019</b></p> <p>This bill requires the share of federal funds for cancer research that is allocated to pediatric cancer research to equal the percentage of the U.S. population that is under the age of 18.</p>]]></text>
          <updateDate>2019-09-20T04:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
