      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2019-09-25</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2020-05-13T12:50:51Z</lastSummaryUpdateDate>
          <text><![CDATA[<p>This resolution accepts the findings in the &quot;Special Report on the Ocean and Cryosphere in a Changing Climate&quot; published by the Intergovernmental Panel on Climate Change and affirms that immediate action is needed to reduce greenhouse gas emissions to protect the ocean and cryosphere.</p>]]></text>
          <updateDate>2019-09-25T04:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
